{
	"name": "Running Tools",
	"version": "1.0.2",
	"description": "Just a calculator to calculate running pace based on distance and time.",
	"manifest_version": 2,
	"icons": {
		"16": "icons/icont_19.png",
		"48": "icons/icon_48.png",
		"128": "icons/icon_128.png"
	},
	"permissions": ["storage"],
	"background": {
		"scripts": ["js/background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_icon": "icons/icont_19.png",
		"default_popup": "index.htm"
	}
}