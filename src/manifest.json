{
	"name": "Runner Tools",
	"version": "1.2.1",
	"description": "Easy way to calculate running pace and calories burned on an activity.",
	"manifest_version": 2,
	"icons": {
		"16": "icons/icont_19.png",
		"48": "icons/icon_48.png",
		"128": "icons/icon_128.png"
	},
	"permissions": [
		"*://*.runner-tools.com/*",
		"storage"
	],
	"background": {
		"scripts": ["js/background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_icon": "icons/icon_19.jpg",
		"default_popup": "index.htm"
	}
	,"web_accessible_resources": [ "icons/icont_19.png", "icons/icon_48.png", "icons/icon_128.png" ]
}